#include <stdio.h>
int main()
{
    int a,b,c;
    printf("enter 3 sides of the triangle:");
    scanf("%d %d %d",&a,&b,&c);
    if (a==b && b==c && a==c){
        printf("this is  a equilateral traingle");
    }
    else if(a!=b && b!=c){
        printf("scalene triangle");
    }
    else{
        printf(" equilateral traingle");
    }
}
